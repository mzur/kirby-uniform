{
    "name": "mzur/kirby-form",
    "description": "A form helper for Kirby-based websites, using the Post/Redirect/Get pattern.",
    "type": "library",
    "license": "MIT",
    "keywords": ["kirby", "form"],
    "authors": [
        {
            "name": "Steve Jamesson",
            "email": "steve@evendev.com"
        },
        {
            "name": "Martin Zurowietz",
            "email": "martin@the-inspired-ones.de"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "mzur/kirby-flash": "@beta",
        "getkirby/cms": "dev-develop || >=3.0.0-beta-4.2"
    },
    "autoload": {
        "psr-4": {
            "Jevets\\Kirby\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Jevets\\Kirby\\Form\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "mzur/kirby-defuse-session": "@beta"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/k-next/kirby.git"
        }
    ]
}
